<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Series Start:"
        android:id="@+id/add_floor_series_start"
        android:layout_marginTop="133dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Series end"
        android:layout_marginTop="30dp"
        android:id="@+id/add_floor_series_end"
        android:layout_below="@+id/add_floor_series_start"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Total_sq_feet"
        android:layout_marginTop="30dp"
        android:id="@+id/add_floor_total_sq_feet"
        android:layout_below="@+id/add_floor_series_end"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/add_floor_layout_series_start_edit_txt"
        android:layout_alignBottom="@+id/add_floor_series_start"
        android:layout_toRightOf="@+id/add_floor_total_sq_feet"
        android:inputType="number"
        android:layout_toEndOf="@+id/add_floor_total_sq_feet" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:id="@+id/add_floor_layout_series_end_edit_txt"
        android:layout_above="@+id/add_floor_total_sq_feet"
        android:layout_toRightOf="@+id/add_floor_total_sq_feet"
        android:layout_toEndOf="@+id/add_floor_total_sq_feet" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:id="@+id/add_floor_layout_total_sq_feet_edit_txt"
        android:layout_alignBottom="@+id/add_floor_total_sq_feet"
        android:layout_toRightOf="@+id/add_floor_total_sq_feet"
        android:layout_toEndOf="@+id/add_floor_total_sq_feet" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Floor"
        android:layout_marginTop="30dp"
        android:id="@+id/add_floor_layout_add_btn"
        android:layout_below="@+id/add_floor_layout_total_sq_feet_edit_txt"
        android:layout_marginLeft="55dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/add_floor_layout_cancel_btn"
        android:layout_alignBottom="@+id/add_floor_layout_add_btn"
        android:layout_toRightOf="@+id/add_floor_layout_add_btn"
        android:layout_toEndOf="@+id/add_floor_layout_add_btn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Add Floor"
        android:id="@+id/textView10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Floor_name"
        android:id="@+id/textView11"
        android:layout_above="@+id/add_floor_layout_series_start_edit_txt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/add_floor_layout_floor_name_edit_text"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignLeft="@+id/add_floor_layout_series_start_edit_txt"
        android:layout_alignStart="@+id/add_floor_layout_series_start_edit_txt" />

</RelativeLayout>